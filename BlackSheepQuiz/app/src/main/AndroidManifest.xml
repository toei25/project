<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.blacksheepquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_sheep"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectCampaignActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SelectTimeattackActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HowtoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StartTimeAttackActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResultScoreActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".StartCampaignActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ResultStageActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
